{
  "manifest_version": 3,
  "name": "NostrNet",
  "short_name": "NostrNet",
  "start_url": "/",
  "display": "standalone",
  "theme_color": "#000000",
  "background_color": "#000000",
  "icons": [
    {
      "src": "/logo192.png",
      "sizes": "64x64 32x32 24x24 16x16",
      "type": "image/x-icon"
    },
    {
      "src": "/logo192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/logo512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "web_accessible_resources": [
  {
    "resources": [
      "scripts/*.js",
      "nostr-provider.js"
    ],
    "matches": [
      "<all_urls>"
    ],
    "use_dynamic_url": true
  }
],
  "permissions": [
    "tabs",
    "storage",
    "webNavigation"
  ],
  "content_scripts": [

  ],
  "background": {

  }

}
